    "prettier": "npx prettier **/*.{md,html} -w",
    "lint": "eslint **/*.js",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flybywind/private_emb_lib"
  },
  "keywords": [],
  "author": "flybywind",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/flybywind/private_emb_lib/issues"
  },
  "homepage": "https://github.com/flybywind/private_emb_lib#readme",
  "devDependencies": {
    "eslint": "^8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.2",
    "prettier": "2.8.4"
  },
  "lint-staged": {
    "**/*.js": [
      "npx eslint --fix"
    ],
    "**/*.{md,html}": [
      "npx prettier --write"
